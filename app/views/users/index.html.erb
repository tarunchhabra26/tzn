<div align="center">
<p id="notice"><%= notice %></p>

<h1>List of Admins</h1>

<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user, :style => "color:#0000FF"  %></td>
        <!td><!%= link_to 'Edit', edit_user_path(user)%><!/td>
        <td><%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' }, :style => "color:#0000FF" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% if isadmin%>
      <button type="button" class="btn"> <%= link_to 'Create New Admin', new_user_path %></button>
<%end%>


